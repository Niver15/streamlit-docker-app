    "import pandas as pd\n",
    "\n",
    "# STEP 1: Load both datasets\n",
    "import pandas as pd\n",
    "\n",
    "# STEP 1: Load both datasets\n",
    "owid = pd.read_csv(r'C:\\Users\\LENO M3\\OneDrive\\Desktop\\owid.csv')\n",
    "OxCGRT = pd.read_csv(r\"C:\\Users\\LENO M3\\OneDrive\\Desktop\\OxCGRT.csv\",encoding='ISO-8859-1')\n",
    "\n",
    "# STEP 2: Clean up country names and date formats\n",
    "owid['location'] = owid['location'].str.strip().str.lower()\n",
    "OxCGRT['CountryName'] = OxCGRT['CountryName'].str.strip().str.lower()\n",
    "#convert dates\n",
    "OxCGRT['Date'] = pd.to_datetime(OxCGRT['Date'], format='%Y%m%d', errors='coerce')\n",
    "owid['date'] = pd.to_datetime(owid['date'], errors='coerce')\n",
    "\n",
    "# STEP 3: Rename columns to match for merging\n",
    "OxCGRT.rename(columns={'CountryName': 'location', 'Date': 'date'}, inplace=True)\n",
    "\n",
    "# STEP 4: Merge datasets safely\n",
    "data = pd.merge(owid, OxCGRT, on=['location', 'date'], how='inner')\n",
    "\n",
    "# STEP 5: Confirm success\n",
    "print(\"✅ Merged dataset successfully!\")\n",
    "print(\"Merged shape:\", data.shape)\n",
    "print(\"Columns:\", data.columns[:10])  # Show first 10 columns\n",
    "print(\"Sample rows:\")\n",
    "print(data[['location', 'date']].head())\n",
    "\n"
   ]
