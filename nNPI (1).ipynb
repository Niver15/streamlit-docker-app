    "#STEP 4:FAIRNESS AUDIT\n",
    "import matplotlib.pyplot as plt\n",
    "import seaborn as sns\n",
    "\n",
    "# Add region info if available, else use location as proxy\n",
    "# Example: Check prediction errors across countries or regions\n",
    "\n",
    "y_pred = model.predict(X_test).flatten()\n",
    "errors = abs(y_test - y_pred)\n",
    "\n",
    "# Assuming 'location' in test data aligned accordingly (simplify example)\n",
    "test_locations = data.iloc[y_test.index]['location'] if hasattr(y_test, 'index') else data['location'].sample(len(y_test))\n",
    "\n",
    "# Plot error distribution by location (only top 5 countries with most data)\n",
    "top_countries = test_locations.value_counts().index[:5]\n",
    "subset = pd.DataFrame({'location': test_locations, 'error': errors})\n",
    "subset = subset[subset['location'].isin(top_countries)]\n",
    "\n",
    "plt.figure(figsize=(12,6))\n",
    "sns.boxplot(data=subset, x='location', y='error')\n",
    "plt.title('Prediction Errors by Location (Fairness Check)')\n",
    "plt.show()\n"
   ]
