   "source": [
    "#STEP 2:DATA PREPROCESSING\n",
    "\n",
    "# Select relevant NPI columns related to health and containment\n",
    "npi_cols = ['S1_School closing', 'S2_Workplace closing', 'S3_Cancel public events',\n",
    "            'S4_Close public transport', 'S5_Public information campaigns']\n",
    "\n",
    "# Fill missing values in NPIs with zeros\n",
    "data[npi_cols] = data[npi_cols].fillna(0)\n",
    "\n",
    "# Define target: reduction in new cases next week (shifted)\n",
    "data['target'] = data['new_cases'].shift(-7)\n",
    "\n",
    "# Drop rows with missing target\n",
    "data.dropna(subset=['target'], inplace=True)\n",
    "\n",
    "print(data[npi_cols + ['target']].head())\n"
   ]
  },
